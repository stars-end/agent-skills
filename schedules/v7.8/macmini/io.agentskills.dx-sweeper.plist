<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>io.agentskills.dx-sweeper</string>
    <key>ProgramArguments</key>
    <array>
        <string>__HOME__/agent-skills/scripts/dx-job-wrapper.sh</string>
        <string>dx-sweeper</string>
        <string>--</string>
        <string>__HOME__/agent-skills/scripts/dx-sweeper.sh</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>2</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>__HOME__/logs/dx/dx-sweeper.launchd.log</string>
    <key>StandardErrorPath</key>
    <string>__HOME__/logs/dx/dx-sweeper.launchd.log</string>
</dict>
</plist>
